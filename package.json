{
  "name": "block-cache",
  "version": "1.0.0",
  "description": "fs read (incl. stream) operations cached in an lru-cache for consistent memory usage with custom fs support (hyperdrive).",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "standard",
    "test:unit": "tap test/*.js",
    "test:ci": "npm run test:lint && npm run test:unit -- --cov --coverage-report=lcov && codeclimate-test-reporter < ./coverage/lcov.info",
    "release": "standard-version"
  },
  "keywords": [
    "hyperdrive",
    "fs",
    "read",
    "cache",
    "lru-cache",
    "dat",
    "parser",
    "dat"
  ],
  "author": "Martin Heidegger <martin.heidegger@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "bluebird": "^3.5.1",
    "hyperdrive": "^9.12.3",
    "random-access-http": "^2.0.0",
    "random-access-memory": "^2.4.0",
    "standard": "^11.0.0",
    "standard-version": "^4.3.0",
    "stream-to-promise": "^2.2.0",
    "tap": "^11.1.1"
  },
  "dependencies": {
    "lru-cache": "^4.1.1",
    "readable-stream": "^2.3.4"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinheidegger/block-cache.git"
  },
  "bugs": {
    "url": "https://github.com/martinheidegger/block-cache/issues"
  },
  "homepage": "https://github.com/martinheidegger/block-cache#readme"
}
